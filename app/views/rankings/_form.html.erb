<%= form_for @ranking do |f| %>
	<% if @ranking.errors.any? %>
	<div id = "error_explanation" >
		<h2>
			<%= pluralize(@ranking.errors.count, "error") %> prohibited this ranking from being saved:
		</h2>
			<ul>
				<% @ranking.errors.full_messages.each do |msg| %>
					<li><%= msg %></li>
					<% end %>
			</ul>
	</div>

<% end %>

	<p>
		<%= f.label :win %>
		<%= f.text_field :win %>
	</p>

	<p>
		<%= f.label :lose %>
		<%= f.text_field :lose %>
	</p>
	<p>
		<%= f.label :tie %>
		<%= f.text_field :tie %>
	</p>

	<p>
		<%= f.label :team_id %>
		<%= f.text_field :team_id %>
	</p>

	<p>
		<%= f.submit %>
	</p>

<% end %>